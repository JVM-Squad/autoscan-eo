+package org.eolang
+home https://github.com/objectionary/eo
+version 0.23.11
+alias org.eolang.hamcrest.assert-that
+junit

[] > converts-to-string
  assert-that > @
    3.1415926.as-string
    $.equal-to "3.1415926"

[] > less-true
  assert-that > @
    -10.5
    $.less-than 50.3

[] > less-equal
  assert-that > @
    -10.7
    $.not
      $.less-than -10.7

[] > less-false
  assert-that > @
    10.13
    $.not
      $.less-than -5.3

[] > greater-true
  assert-that > @
    assert-that
      -200.1
      $.greater-than -1000.0
    $.anything

[] > greater-false
  assert-that > @
    assert-that
      0.5
      $.not
        $.greater-than 100.1
    $.anything

[] > greater-equal
  assert-that > @
    assert-that
      0.7
      $.not
        $.greater-than 0.7
    $.anything

[] > leq-true
  assert-that > @
    lte.
      -200.5
      -100.6
    $.equal-to TRUE

[] > leq-equal
  assert-that > @
    lte.
      50.1
      50.1
    $.equal-to TRUE

[] > leq-false
  assert-that > @
    lte.
      0.9
      -10.11
    $.equal-to FALSE

[] > gte-true
  assert-that > @
    gte.
      -1000.1
      -1100.1
    $.equal-to TRUE

[] > gte-equal
  assert-that > @
    gte.
      113.333
      113.333
    $.equal-to TRUE

[] > gte-false
  assert-that > @
    gte.
      0.7
      10.9
    $.equal-to FALSE

[] > eq-true
  assert-that > @
    123.5
    $.equal-to 123.5

[] > eq-false
  assert-that > @
    123.5
    $.not
      $.equal-to 42.4

[] > compares-two-different-types
  assert-that > @
    3.14
    $.not
      $.equal-to "Hello"

[] > neq-true
  assert-that > @
    neq.
      123.5
      125.2
    $.equal-to TRUE

[] > neq-false
  assert-that > @
    neq.
      -133.9
      -133.9
    $.not
      $.equal-to TRUE

[] > times-by-zero
  assert-that > @
    times.
      574.9
      0.0
    $.equal-to 0.0

[] > times-by-one
  assert-that > @
    times.
      574.9
      1.0
    $.equal-to 574.9

[] > times-two-positives
  assert-that > @
    times.
      574.5
      311.985
    $.equal-to 179235.3825

[] > times-two-negatives
  assert-that > @
    times.
      -574.5
      -311.985
    $.equal-to 179235.3825

[] > times-diff-sign
  assert-that > @
    times.
      -0.5745
      0.311985
    $.equal-to -0.1792353825

[] > add-test
  assert-that > @
    plus.
      574.5
      311.985
    $.equal-to 886.485

[] > neg-test
  assert-that > @
    886.485.neg
    $.equal-to -886.485

[] > signum-negative
  assert-that > @
    -345.17.signum
    $.equal-to -1.0

[] > signum-positive
  assert-that > @
    145.18.signum
    $.equal-to 1.0

[] > signum-zero
  assert-that > @
    0.0.signum
    $.equal-to 0.0

[] > sub-test
  assert-that > @
    minus.
      574.5
      311.985
    $.equal-to 262.515

[] > div-test
  assert-that > @
    div.
      15.5
      0.5
    $.equal-to 31.0

[] > abs-positive
  assert-that > @
    13.5.abs
    $.equal-to 13.5

[] > abs-negative
  assert-that > @
    -17.9.abs
    $.equal-to 17.9

[] > abs-zero
  assert-that > @
    0.0.abs
    $.equal-to 0.0

[] > as-int-works
  assert-that > @
    3.57.as-int
    $.equal-to 3

[] > sin-zero
  assert-that > @
    0.0.sin
    $.equal-to 0.0

[] > sin-pi-div-2
  assert-that > @
    1.5707963267948966.sin
    $.equal-to 1.0

[] > sin-pi-as-int
  assert-that > @
    (3.141592653589793.sin).as-int
    $.equal-to 0

[] > cos-zero
  assert-that > @
    0.0.cos
    $.equal-to 1.0

[] > cos-pi-div-2-as-int
  assert-that > @
    (1.5707963267948966.cos).as-int
    $.equal-to 0

[] > cos-pi
  assert-that > @
    3.141592653589793.cos
    $.equal-to -1.0

[] > to-bytes-and-backwards
  assert-that > @
    as-float.
      as-bytes.
        3.1415926
    $.equal-to 3.1415926

[] > as-bytes-equals-to-float
  assert-that > @
    3.1415926.as-bytes
    $.equal-to 3.1415926

[] > as-bytes-equals-to-float-backwards
  assert-that > @
    3.1415926.as-bytes
    $.equal-to 3.1415926

[] > similar-values-have-similar-hash
  assert-that > @
    (3.1415926).as-hash
    $.equal-to
      (3.1415926).as-hash
